<?xml version="1.0" encoding="UTF-8"?>
<elements>
	<dragelemcategory name="motion" color="#4a6cd4">
		<dragableelement name="forward" type="command" spec="move %n steps">
			<parameter default="10" />
		</dragableelement>
		<dragableelement name="turn" type="command" spec="turn %Pixmap:/Assets/clockwise.png %n degrees">
			<parameter default="15" />
		</dragableelement>
		<dragableelement name="turnLeft" type="command" spec="turn %Pixmap:/Assets/counterclockwise.png %n degrees">
			<parameter default="15" />
		</dragableelement>
		<dragableelement name="setHeading" type="command" spec="point in direction %com">
			<parameter>
				<option value="right" />
				<option value="left" />
				<option value="up" />
				<option value="down" />
			</parameter>
		</dragableelement>
		<dragableelement name="doFaceTowards" type="command" spec="point towards %com" />
		<dragableelement name="gotoXY" type="command" spec="go to x: %n y: %n">
			<parameter default="0" />
			<parameter default="0" />
		</dragableelement>
		<dragableelement name="doGotoObject" type="command" spec="go to %com" />
		<dragableelement name="doGlide" type="command" spec="glide %n secs to x: %n y: %n">
			<parameter default="1" />
			<parameter default="0" />
			<parameter default="0" />
		</dragableelement>
		<dragableelement name="changeXPosition" type="command" spec="change x by %n">
			<parameter default="10" />
		</dragableelement>
		<dragableelement name="setXPosition" type="command" spec="set x to %n">
			<parameter default="0" />
		</dragableelement>
		<dragableelement name="changeYPosition" type="command" spec="change y by %n">
			<parameter default="10" />
		</dragableelement>
		<dragableelement name="setYPosition" type="command" spec="set y to %n">
			<parameter default="0" />
		</dragableelement>
		<dragableelement name="bounceOffEdge" type="command" spec="if on edge, bounce" />
		<dragableelement name="xPosition" type="reporter" spec="x position" />
		<dragableelement name="yPosition" type="reporter" spec="y position" />
		<dragableelement name="direction" type="reporter" spec="direction" />
	</dragelemcategory>
	
	<dragelemcategory name="looks" color="#8f56e3">
		<dragableelement name="doSwitchToCostume" type="command" spec="switch to costume %com" />
		<dragableelement name="doWearNextCostume" type="command" spec="next costume" />
		<dragableelement name="getCostumeIdx" type="reporter" spec="costume #" />
		<dragableelement name="doSayFor" type="command" spec="say %s for %n secs">
			<parameter default="0" />
			<parameter default="2" />
		</dragableelement>
		<dragableelement name="bubble" type="command" spec="say %s">
			<parameter default="0" />
		</dragableelement>
		<dragableelement name="doThinkFor" type="command" spec="think %s for %n secs">
			<parameter default="0" />
			<parameter default="2" />
		</dragableelement>
		<dragableelement name="doThink" type="command" spec="think %s">
			<parameter default="0" />
		</dragableelement>
		<dragableelement name="changeEffect" type="command" spec="change %com effect by %n">
			<parameter>
				<option value="ghost" />
				<option value="brightness" />
				<option value="negative" />
				<option value="comic" />
				<option value="duplicate" />
				<option value="confetti" />
			</parameter>
			<parameter default="25" />
		</dragableelement>
		<dragableelement name="setEffect" type="command" spec="set %com effect to %n">
			<parameter>
				<option value="ghost" />
				<option value="brightness" />
				<option value="negative" />
				<option value="comic" />
				<option value="duplicate" />
				<option value="confetti" />
			</parameter>
			<parameter default="0" />
		</dragableelement>
		<dragableelement name="clearEffects" type="command" spec="clear graphic effects" />
		<dragableelement name="changeScale" type="command" spec="change size by %n">
			<parameter default="10" />
		</dragableelement>
		<dragableelement name="setScale" type="command" spec="set size to %n %">
			<parameter default="100" />
		</dragableelement>
		<dragableelement name="getScale" type="reporter" spec="size" />
		<dragableelement name="show" type="command" spec="show" />
		<dragableelement name="hide" type="command" spec="hide" />
		<dragableelement name="comeToFront" type="command" spec="go to front" />
		<dragableelement name="goBack" type="command" spec="go back %n layers">
			<parameter default="1" />
		</dragableelement>
		<dragableelement name="reportCostumes" type="reporter" spec="wardrobe" />
		<dragableelement name="log" type="command" spec="console log %mult%s" />
	</dragelemcategory>
	
	<dragelemcategory name="sound" color="#cf4ad9">
		<dragableelement name="playSound" type="command" spec="play sound %com" />
		<dragableelement name="doPlaySoundUntilDone" type="command" spec="play sound %com until done" />
		<dragableelement name="doStopAllSounds" type="command" spec="stop all sounds" />
		<dragableelement name="doRest" type="command" spec="rest for %n beats">
			<parameter default="0.2" />
		</dragableelement>
		<dragableelement name="doPlayNote" type="command" spec="play note %n for %n beats">
			<parameter default="60" />
			<parameter default="0.5" />
		</dragableelement>
		<dragableelement name="doChangeTempo" type="command" spec="change tempo by %n">
			<parameter default="20" />
		</dragableelement>
		<dragableelement name="doSetTempo" type="command" spec="set tempo to %n bpm">
			<parameter default="60" />
		</dragableelement>
		<dragableelement name="getTempo" type="reporter" spec="tempo" />
		<dragableelement name="reportSounds" type="reporter" spec="jukebox" />
	</dragelemcategory>
	
	<dragelemcategory name="pen" color="#00a178">
		<dragableelement name="clear" type="command" spec="clear" />
			<dragableelement name="penDown" type="command" spec="pen down" />
			<dragableelement name="penUp" type="command" spec="pen up" />
		<dragableelement name="setColor" type="command" spec="set pen color to %clr" />
		<dragableelement name="changeHue" type="command" spec="change pen color by %n">
			<parameter default="10" />
		</dragableelement>
		<dragableelement name="setHue" type="command" spec="set pen color to %n">
			<parameter default="0" />
		</dragableelement>
		<dragableelement name="changeBrightness" type="command" spec="change pen shade by %n">
			<parameter default="10" />
		</dragableelement>
		<dragableelement name="setBrightness" type="command" spec="set pen shade to %n">
			<parameter default="100" />
		</dragableelement>
		<dragableelement name="changeSize" type="command" spec="change pen size by %n">
			<parameter default="1" />
		</dragableelement>
		<dragableelement name="setSize" type="command" spec="set pen size to %n">
			<parameter default="1" />
		</dragableelement>
		<dragableelement name="doStamp" type="command" spec="stamp" />
	</dragelemcategory>
	
	<dragelemcategory name="control" color="#e6a822">
		<dragableelement name="receiveGo" type="hat" spec="when %Pixmap:/Assets/greenFlagOn.png clicked" />
		<dragableelement name="receiveKey" type="hat" spec="when %com key pressed" />
		<dragableelement name="receiveInteraction" type="hat" spec="when I am %com">
			<parameter>
				<option value="clicked" />
				<option value="dragged" />
				<option value="dropped" />
				<option value="hover" />
			</parameter>
		</dragableelement>
		<dragableelement name="receiveMessage" type="hat" spec="when I receive %com" />
		<dragableelement name="doBroadcast" type="command" spec="broadcast %com" />
		<dragableelement name="doBroadcastAndWait" type="command" spec="broadcast %com and wait" />
		<dragableelement name="getLastMessage" type="reporter" spec="message" />
		<dragableelement name="doWait" type="command" spec="wait %n secs">
			<parameter default="1" />
		</dragableelement>
		<dragableelement name="doWaitUntil" type="command" spec="wait until %b" />
		<dragableelement name="doForever" type="wrapper" spec="forever %c" />
		<dragableelement name="doRepeat" type="wrapper" spec="repeat %n %c">
			<parameter default="10" />
		</dragableelement>
		<dragableelement name="doUntil" type="wrapper" spec="repeat until %b %c" />
		<dragableelement name="doIf" type="wrapper" spec="if %b %c" />
		<dragableelement name="doIfElse" type="wrapper" spec="if %b %c else %c" />
		<dragableelement name="doStopThis" type="command" spec="stop %com">
			<parameter>
				<option value="all" />
				<option value="this Script" />
				<option value="this Block" />
				<option value="all but this Script" />
				<option value="other Scripts in Sprite" />
			</parameter>
		</dragableelement>
		<dragableelement name="receiveOnClone" type="hat" spec="when I start as a clone" />
		<dragableelement name="createClone" type="command" spec="create a clone of %com">
			<parameter model="sprite"/>
		</dragableelement>
		<dragableelement name="removeClone" type="command" spec="delete this clone" />
	</dragelemcategory>
	
	<dragelemcategory name="sensing" color="#0494dc">
		<dragableelement name="reportTouchingObject" type="predicate" spec="touching %com ?" />
		<dragableelement name="reportTouchingColor" type="predicate" spec="touching %clr ?" />
		<dragableelement name="reportColorIsTouchingColor" type="predicate" spec="color %clr is touching %clr ?" />
		<dragableelement name="colorFiltered" type="reporter" spec="filtered for %clr" />
		<dragableelement name="reportStackSize" type="reporter" spec="stack size" />
		<dragableelement name="reportFrameCount" type="reporter" spec="frames" />
		<dragableelement name="reportThreadCount" type="reporter" spec="processes" />
		<dragableelement name="doAsk" type="command" spec="ask %s and wait">
			<parameter default="0" />
		</dragableelement>
		<dragableelement name="reportLastAnswer" type="reporter" spec="answer" />
		<dragableelement name="getLastAnswer" type="reporter" spec="answer" />
		<dragableelement name="reportMouseX" type="reporter" spec="mouse x" />
		<dragableelement name="reportMouseY" type="reporter" spec="mouse y" />
		<dragableelement name="reportMouseDown" type="predicate" spec="mouse down?" />
		<dragableelement name="reportKeyPressed" type="predicate" spec="key %com pressed?" />
		<dragableelement name="reportDistanceTo" type="reporter" spec="distance to %com" />
		<dragableelement name="doResetTimer" type="command" spec="reset timer" />
		<dragableelement name="reportTimer" type="reporter" spec="timer" />
		<dragableelement name="getTimer" type="reporter" spec="timer" />
		<dragableelement name="reportURL" type="reporter" spec="http:// %s">
			<parameter default="snap.berkeley.edu" />
		</dragableelement>
		<dragableelement name="reportIsFastTracking" type="predicate" spec="turbo mode?" />
		<dragableelement name="doSetFastTracking" type="command" spec="set turbo mode to %b" />
	</dragelemcategory>
	
	<dragelemcategory name="operators" color="#62c213">
		<dragableelement name="reportSum" type="reporter" spec="%n + %n" />
		<dragableelement name="reportDifference" type="reporter" spec="%n - %n" />
		<dragableelement name="reportProduct" type="reporter" spec="%n * %n" />
		<dragableelement name="reportQuotient" type="reporter" spec="%n / %n" />
		<dragableelement name="reportRound" type="reporter" spec="round %n" />
		<dragableelement name="reportFunction" type="reporter" spec="%com of %n">
			<parameter>
				<option value="abs" />
				<option value="floor" />
				<option value="ceil" />
				<option value="sqrt" />
				<option value="sin" />
				<option value="cos" />
				<option value="tan" />
				<option value="asin" />
				<option value="acos" />
				<option value="atan" />
				<option value="ln" />
				<option value="e^" />
			</parameter>
			<parameter default="10" />
		</dragableelement>
			<dragableelement name="reportModulo" type="reporter" spec="%n mod %n" />
			<dragableelement name="reportLessThan" type="predicate" spec="%n &lt; %n" />
		<dragableelement name="reportRandom" type="reporter" spec="pick random %n to %n">
			<parameter default="1" />
			<parameter default="10" />
		</dragableelement>
			<dragableelement name="reportEquals" type="predicate" spec="%n = %n" />
			<dragableelement name="reportGreaterThan" type="predicate" spec="%n &gt; %n" />
		<dragableelement name="reportAnd" type="predicate" spec="%b and %b" />
		<dragableelement name="reportOr" type="predicate" spec="%b or %b" />
		<dragableelement name="reportNot" type="predicate" spec="not %b" />
		<dragableelement name="reportTrue" type="predicate" spec="true" />
		<dragableelement name="reportFalse" type="predicate" spec="false" />
		<dragableelement name="reportJoinWords" type="reporter" spec="join %s %s">
			<parameter default="0 " />
			<parameter default="0" />
		</dragableelement>
		<dragableelement name="reportLetter" type="reporter" spec="letter %n of %s">
			<parameter default="1" />
			<parameter default="0" />
		</dragableelement>
		<dragableelement name="reportStringSize" type="reporter" spec="length of %s">
			<parameter default="0" />
		</dragableelement>
		<dragableelement name="reportUnicode" type="reporter" spec="unicode of %s">
			<parameter default="a" />
		</dragableelement>
		<dragableelement name="reportUnicodeAsLetter" type="reporter" spec="unicode %n as letter">
			<parameter default="65" />
		</dragableelement>
		<dragableelement name="reportIsA" type="predicate" spec="is %s a %com ?">
			<parameter default="5" />
			<parameter>
				<option value="number" />
				<option value="text" />
				<option value="boolean" />
				<option value="list" />
				<option value="command" />
				<option value="reporter" />
				<option value="predicate" />
			</parameter>
		</dragableelement>
		<dragableelement name="reportIsIdentical" type="predicate" spec="is %s identical to %s ?" />
		<dragableelement name="reportTextSplit" type="reporter" spec="split %s by %com">
			<parameter default="0 0" />
			<parameter>
				<option value="letter" />
				<option value="whitespace" />
				<option value="line" />
				<option value="tab" />
				<option value="cr" />
			</parameter>
		</dragableelement>
		<!--<dragableelement name="reportJSFunction" type="reporter" spec="JavaScript function ( %mult%s ) { %code }" /> -->
		<dragableelement name="reportTypeOf" type="reporter" spec="type of %s">
			<parameter default="5" />
		</dragableelement>
	</dragelemcategory>
	
	<dragelemcategory name="variables" color="#f3761d">
		<dragableelement name="doSetVar" type="command" spec="set %com to %s">
			<parameter default="null" />
			<parameter default="0" />
		</dragableelement>
		<dragableelement name="doChangeVar" type="command" spec="change %com by %n">
			<parameter default="null" />
			<parameter default="1" />
		</dragableelement>
		<dragableelement name="doShowVar" type="command" spec="show variable %com" />
		<dragableelement name="doHideVar" type="command" spec="hide variable %com" />
		<dragableelement name="reportCONS" type="reporter" spec="%s in front of %com" />
		<dragableelement name="reportListItem" type="reporter" spec="item %com of %com">
			<parameter default="1" />
		</dragableelement>
		<dragableelement name="reportCDR" type="reporter" spec="all but first of %com" />
		<dragableelement name="reportListLength" type="reporter" spec="length of %com" />
		<dragableelement name="reportListContainsItem" type="predicate" spec="%com contains %s">
			<parameter default="null" />
			<parameter default="0" />
		</dragableelement>
		<dragableelement name="doAddToList" type="command" spec="add %s to %com">
			<parameter default="0" />
		</dragableelement>
		<dragableelement name="doDeleteFromList" type="command" spec="delete %com of %com">
			<parameter default="1" />
		</dragableelement>
		<dragableelement name="doInsertInList" type="command" spec="insert %s at %com of %com">
			<parameter default="0" />
			<parameter default="1" />
		</dragableelement>
		<dragableelement name="doReplaceInList" type="command" spec="replace item %com of %com with %s">
			<parameter default="1" />
			<parameter default="null" />
			<parameter default="0" />
		</dragableelement>
		<!--
		<dragableelement name="reportMap" type="reporter" spec="map %repRing over %com" />
		<dragableelement name="doForEach" type="command" spec="for %upvar in %com %cs"> 
			<parameter default="0" />
		</dragableelement>
		-->
	</dragelemcategory>
	
